{
  "name": "mymo-cli",
  "version": "0.0.0-development",
  "description": "A tool to help generate things for Mymoid",
  "main": "dist/index.js",
  "scripts": {
    "add-contributor": "kcd-scripts contributors add",
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|graphql|md|mdx)\"",
    "test": "kcd-scripts test --coverageThreshold \"{\\\"global\\\": {}}\"",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "precommit": "kcd-scripts precommit",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "mymo-cli": "dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Denis Alonso <denjalonso@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "chalk": "^2.4.2",
    "common-tags": "^1.8.0",
    "glob": "^7.0.6",
    "mkdirp": "^0.5.1",
    "p-limit": "^1.1.0",
    "pify": "^2.3.0",
    "rimraf": "^2.7.1",
    "semantic-release": "17.0.1",
    "simple-git": "^1.131.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "directory-tree": "^1.1.0",
    "kcd-scripts": "^0.2.3",
    "prettier": "^1.19.1",
    "spawn-command": "0.0.2",
    "strip-ansi": "^4.0.0",
    "yargs-parser": "^5.0.0",
    "semantic-release": "^17.0.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "babel": {
    "presets": "kcd-scripts/babel",
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technoactivity/mymo-cli.git"
  }
}
